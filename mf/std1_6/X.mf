% MetaFlop
% Standard 1.3

beginadjchar("X",5, cap#, 0);



x1l=leftstemloc-cont*px/2;
x2r=w-x1l;
x3l=x1l;
x4r=x2+.5(x2r-x2);

y1=y4=h;
y2=y3=0;



alpha1=diag_ratio(1,cont*px,h,x2r-x1l);
alpha2=diag_ratio(1,px,h,x2r-x1l);
penpos1(alpha1*cont*px,0);
penpos2(alpha1*cont*px,0);
penpos3(alpha2*px,0);
penpos4(alpha2*px,0);


penstroke z1e..z2e;
penstroke z3e..z4e;

% penlabels(range 1 thru 30);

endchar;
